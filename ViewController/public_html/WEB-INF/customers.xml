<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="customers">
    <default-activity>Customers</default-activity>
    <view id="Customers">
      <page>/Customers.jsf</page>
    </view>
    <view id="login">
      <page>/login.jsf</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>Customers</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>customersBackToLogin</from-outcome>
        <to-activity-id>login</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
